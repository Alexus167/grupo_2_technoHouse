<!DOCTYPE html>
<html lang="en">

<%- include('partials/head') %>

<body>
  <header>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12"></div>
                <a href="/" class="logoA">
                    <img class="logo" src="/images/Logo_.png" alt="">
                </a>
                </div>
         </div>
     </nav>
</header>

    <section>
        <div class="container inicia">
          <div class="row">
            <div class="col-md-12">
              <div class="login-container">
                <div class="register">
                  <h2 class="text1">¡Hola! Para continuar iniciar sesión</h2>
                  <form action="">
                    <input type="text" placeholder="Nombre" class="nombre">
                    <input type="text" placeholder="Correo" class="correo">
                    <input type="password" placeholder="Contraseña" class="pass">
                    <input type="submit" class="submit" value="INICIAR">
                  </form>
                  <a href="#" style="color: #30475e;">
                      <h1 class="noLogin">¿Aún no estás registrado?</h1>
                  </a>
                </div>
                <div class="login">
                  <h2 class="iniciar">Iniciar cuenta con</h2>
                  <div class="login-items">
                    <button class="fb"><i class="fab fa-facebook-f"></i> Acceder con Facebook</button>
                    <button class="tw"><i class="fab fa-twitter"></i> Acceder con Twitter</button>
                    <button class="correo"><i class="fab fa-google-plus-g"></i> Acceder con Correo</button>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
    </section>
   

    <%- include('partials/footer') %>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>
